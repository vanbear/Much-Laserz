            <kind>1</kind>
            <string>var move_speed = 1;

if (!active)
{
    sprite_index=spr_movepanel_off;   
    // zatrzymywanie emittera
    with (obj_emitter)
    {
        if (other.ident==ident)
        {
           path_speed=0;
        }
    }
    
    // zatrzymywanie lustra
    with (obj_mirrortest)
    {
        if (other.ident==ident)
        {
           path_speed=0;
        }
    }
} 
else
{
    if dir sprite_index=spr_movepanel_left; else  sprite_index=spr_movepanel_right;
    
    // przesuwanie emittera
    with (obj_emitter)
    {
        if (other.ident==ident)
        {
           if (other.dir==0)
              path_speed=move_speed;
           if (other.dir==1)
              path_speed=-move_speed;
        }
    }
    
    // przesuwanie lustra
    with (obj_mirrortest)
    {
        if (other.ident==ident)
        {
           if (other.dir==0)
              path_speed=move_speed;
           if (other.dir==1)
              path_speed=-move_speed;
           
        }
        
    }
    
}

if active 
{
    var snd = audio_play_sound(snd_moving,10,0);
    audio_sound_gain(snd, .1,1);
}
