{
    mpos += move;
    if (mpos &lt; 0) mpos = array_length_1d(global.levels) - 1;
    if (mpos &gt; array_length_1d(global.levels) - 1) mpos = 0;
}

var push;
push = max(select_key,0);

if (push == 1) scr_level_select();

if back_key room_goto(rm_menu);
